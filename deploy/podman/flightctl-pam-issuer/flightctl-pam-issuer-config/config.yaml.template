service:
  baseUrl: https://{{BASE_DOMAIN}}:3443/
  baseAgentEndpointUrl: https://{{BASE_DOMAIN}}:7443/
  baseUIUrl: https://{{BASE_DOMAIN}}:443
auth:
  pamOidcIssuer:
    address: ":8444"
    issuer: {{PAM_OIDC_ISSUER}}
    clientId: {{PAM_OIDC_CLIENT_ID}}
    clientSecret: {{PAM_OIDC_CLIENT_SECRET}}
    scopes: {{PAM_OIDC_SCOPES}}
    redirectUris: {{PAM_OIDC_REDIRECT_URIS}}
    pamService: {{PAM_OIDC_SERVICE}}
    allowPublicClientWithoutPKCE: {{PAM_OIDC_ALLOW_PUBLIC_WITHOUT_PKCE}}


