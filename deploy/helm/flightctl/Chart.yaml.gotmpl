  charts.openshift.io/name: {{ .Annotations.Name }}
  charts.openshift.io/provider: {{ .Annotations.Provider }}
  charts.openshift.io/supportURL: {{ .Annotations.SupportURL }}
kubeVersion: '>= 1.32.0-0'
