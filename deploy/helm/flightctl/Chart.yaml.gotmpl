apiVersion: v2
name: {{ .Name }}
description: {{ .Description }}
home: {{ .Home }}
type: application
version: 0.1.0
appVersion: "latest" # this is replaced at packaging with the hash of the image release
icon: {{ .Icon }}
annotations:
